{
  "name": "pokelove",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" --ignore-unknown --no-error-on-unmatched-pattern",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\" --ignore-unknown --no-error-on-unmatched-pattern",
    "prepare": "husky",
    "beforeCommit": "npm run type-check && npm run lint && npm run format"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.2.7",
    "@supabase/supabase-js": "^2.44.3",
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.24.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.0.11",
    "postcss": "^8.4.39",
    "prettier": "3.3.2",
    "supabase": "^1.183.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
